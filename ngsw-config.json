{
  "$schema": "./node_modules/@angular/service-worker/config/schema.json",
  "index": "/index.html",
  "assetGroups": [
    {
      "name": "appshell",
      "installMode": "prefetch",
      "resources": {
        "files": [
          "/favicon.ico",
          "/index.html",
          "/manifest.webmanifest",
          "/styles.*.css",
          "/vendor.*.js",
          "/common.*.js",
          "/main.*.js",
          "/runtime.*.js",
          "/*polyfills.*.js"
        ]
      }
    },
    {
      "name": "modules",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/*.js",
          "!/vendor.*.js",
          "!/common.*.js",
          "!/main.*.js",
          "!/runtime.*.js",
          "!/*polyfills.*.js"
        ]
      }
    },
    {
      "name": "assets",
      "installMode": "lazy",
      "updateMode": "prefetch",
      "resources": {
        "files": [
          "/assets/**/*.(svg|jpg|png|webp|gif)",
          "/svg/**"
        ]
      }
    }
  ]
}
